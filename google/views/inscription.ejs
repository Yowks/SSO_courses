<div class="container">
    <div class="content_div_class">
        <h1><%= title%></h1>
        <p>lead</p>
        <br>

        <form action="register" method="post" style="max-width: 300px" role="form">
            <div class="form-group">
                <input type="text" class="form-control" name="username" placeholder="mail adress">
            </div>
            <div class="form-group">
                <input type="password" class="form-control" name="password" placeholder="password">
            </div>
            <div class="btn-group">
                <button class="btn btn-default waves-effect waves-light" type="submit"> Register </button>
                <a href="/auth/google" class="btn btn-social-icon waves-effect red pull-right" role="button">Google</a>
            </div>
        </form>
    </div>
</div>